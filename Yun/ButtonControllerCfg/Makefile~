CC = cc
LIBS = -lpthread 
CFLAGS = -O2 -w
SRCS = ButtonController.c ButtonControllerCfg.c runtime.c
OBJS = ButtonController.o ButtonControllerCfg.o runtime.o

all : ButtonControllerCfg

.c.o :
	${CC} ${CFLAGS} -c $< 

ButtonControllerCfg : $(OBJS)
	$(CC) -o $@ $(OBJS) $(LIBS) -lm

clean:
	rm -f *.o *~ ButtonControllerCfg
